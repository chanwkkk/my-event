<%= f.label :name %>
<%= f.text_field :name, :value=>user.name%><br>
<%= f.label :email %>
<%= f.text_field :email, :value=>user.email%><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :bio %><br>
<%= f.text_area :bio %><br>

<%= f.label :is_organization %>
<%= f.check_box :is_organization %><br>
<%= f.submit %>