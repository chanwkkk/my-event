<%= form_for @user, :url=>'/create_from_google' do |f|%>
    
    <% if @user.errors.any?%>
    <h2>
      <%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:
    </h2>

    <ul>
    <% @user.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>

<% end %>

    <%= f.label :name %>
    <%= f.text_field :name, :value=>@user.name%>
    <%= f.label :email %>
    <%= f.text_field :email, :value=>@user.email%>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %><br>
 
    <%= f.label :is_organization %>
    <%= f.check_box :is_organization %><br>
    <%= f.submit %>
<% end %>
