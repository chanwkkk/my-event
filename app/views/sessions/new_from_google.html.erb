<%= form_for @user, :url=>'/create_from_google' do |f|%>
    
  <%= render partial: "layouts/error", locals: {object: @user} %>


    <%= f.label :name %>
    <%= f.text_field :name, :value=>@user.name%>
    <%= f.label :email %>
    <%= f.text_field :email, :value=>@user.email%>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %><br>
 
    <%= f.label :is_organization %>
    <%= f.check_box :is_organization %><br>
    <%= f.submit %>
<% end %>
